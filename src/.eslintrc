{
    "parser": "babel-eslint",
    "env":{
        "es6":true,
        "browser": true,
        "node":true
    },
    "rules": {
    "semi":"off",
    "arrow-parens":"off",
    "object-curly-spacing":"off",
    "require-jsdoc":"off",
    "indent":"off",
    "quotes":"off",
    "operator-linebreak":"off"
    },
    "extends": ["eslint:recommended", "google"]
}